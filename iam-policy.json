{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "arn:aws:logs:eu-north-1:470384041530:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:eu-north-1:470384041530:log-group:/aws/lambda/img-ingest-processor:*"
            ]
        }
    ]
}
{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:ListBucket"
			],
			"Resource": [
				"arn:aws:s3:::img-ingest-input",
				"arn:aws:s3:::img-ingest-input/*",
				"arn:aws:s3:::img-ingest-output",
				"arn:aws:s3:::img-ingest-output/*"
			]
		}
	]
}
